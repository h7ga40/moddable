{
	"modules": {
		"pins/analog": "$(MODULES)/pins/analog/analog",
	},
	"preload": "pins/analog",
	"platforms": {
		"esp": {
			"modules": {
				"*": "$(MODULES)/pins/analog/esp/*"
			},
		},
		"esp32": {
			"modules": {
				"*": "$(MODULES)/pins/analog/esp32/*"
			},
		},
		"gr_rose": {
			"modules": {
				"*": "$(MODULES)/pins/analog/gr_rose/*"
			},
		},
		"gecko": {
			"modules": {
				"*": "$(MODULES)/pins/analog/gecko/*"
			},
		},
		"...": {
			"error": "pins/analog module unsupported"
		}
	}
}
