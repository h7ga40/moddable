{
	"modules": {
		"pins/digital": "$(MODULES)/pins/digital/digital",
	},
	"preload": "pins/digital",
	"platforms": {
		"esp": {
			"modules": {
				"*": "$(MODULES)/pins/digital/esp/*",
			},
		},
		"esp32": {
			"modules": {
				"*": "$(MODULES)/pins/digital/esp32/*",
			},
		},
		"gr_rose": {
			"modules": {
				"*": "$(MODULES)/pins/digital/gr_rose/*",
			},
		},
		"gecko": {
			"modules": {
				"*": "$(MODULES)/pins/digital/gecko/*",
			},
		},
		"qca4020": {
			"modules": {
				"*": "$(MODULES)/pins/digital/qca4020/*",
			},
		},
		"...": {
			"error": "pins/digital module unsupported"
		}
	}
}
